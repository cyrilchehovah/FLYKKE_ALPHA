<div class="post-flykke-section">
    <%= link_to flykke_path(id: post.id), class: "btn-flykke", :method => :put do %>
      <%= image_tag("bird-mini.png", :alt => "bird", :class => "logo-mini") %>
      <h3 class="btn-flykke-vote-count"><%= post.get_likes(:vote_scope => 'flykke').size %></h3>
    <% end %>
</div>
